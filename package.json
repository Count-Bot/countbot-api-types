{
  "name": "countbot-api-types",
  "version": "1.0.0",
  "description": "API types for CountBot",
  "exports": {
    "./payloads": {
      "types": "./build/payloads/index.d.ts",
      "require": "./build/payloads/index.js",
      "import": "./build/payloads/index.mjs"
    },
    "./rest": {
      "types": "./build/rest/index.d.ts",
      "require": "./build/rest/index.js",
      "import": "./build/rest/index.mjs"
    },
    "./utils": {
      "types": "./build/utils/index.d.ts",
      "require": "./build/utils/index.js",
      "import": "./build/utils/index.mjs"
    }
  },
  "author": {
    "name": "CountBot",
    "email": "realcountbot@gmail.com",
    "url": "https://count.bot"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Count-Bot/countbot-api-types/issues"
  },
  "homepage": "https://github.com/Count-Bot/countbot-api-types#readme",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.2.12",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint --cache --fix",
    "check": "npm run lint && tsc --noEmit",
    "prepare": "npm run build",
    "preinstall": "npm run build"
  },
  "dependencies": {
    "mongoose": "^8.4.3"
  }
}