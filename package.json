{
  "name": "countbot-api-types",
  "version": "1.0.0",
  "description": "API types for CountBot",
  "exports": {
    "./payloads": {
      "types": "./payloads/index.d.ts",
      "require": "./payloads/index.js",
      "import": "./payloads/index.mjs"
    },
    "./rest": {
      "types": "./rest/index.d.ts",
      "require": "./rest/index.js",
      "import": "./rest/index.mjs"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "require": "./utils/index.js",
      "import": "./utils/index.mjs"
    },
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.mjs"
    }
  },
  "author": {
    "name": "CountBot",
    "email": "realcountbot@gmail.com",
    "url": "https://count.bot"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Count-Bot/countbot-api-types/issues"
  },
  "homepage": "https://github.com/Count-Bot/countbot-api-types#readme",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.2.12",
    "gen-esm-wrapper": "^1.1.3",
    "npm-run-all2": "^6.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  },
  "scripts": {
    "build": "tsc && run-p esm:*",
    "lint": "eslint --cache --fix",
    "check": "npm run lint && tsc --noEmit",
    "prepare": "npm run build",
    "preinstall": "npm run build",
    "esm:payloads": "gen-esm-wrapper ./payloads/index.js ./payloads/index.mjs",
    "esm:rest": "gen-esm-wrapper ./rest/index.js ./rest/index.mjs",
    "esm:utils": "gen-esm-wrapper ./utils/index.js ./utils/index.mjs",
    "esm:index": "gen-esm-wrapper ./index.js ./index.mjs"
  },
  "dependencies": {
    "mongoose": "^8.4.3"
  }
}